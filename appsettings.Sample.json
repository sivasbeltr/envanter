{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Keycloak": {
    "ServerRealm": "https://login.domain.com.tr/realms/myrealm",
    "Metadata": "https://login.domain.com.tr/realms/myrealm/.well-known/openid-configuration",
    "ClientId": "envanter",
    "ClientSecret": "secretkey",
    "CallbackPath": "/signin-oidc",
    "SignedOutCallbackPath": "/signout-callback-oidc",
    "Authority": "https://login.domain.com.tr/realms/myrealm",
    "ResponseType": "code",
    "SaveTokens": true,
    "RequireHttpsMetadata": true,
    "GetClaimsFromUserInfoEndpoint": true,
    "Scope": [
      "openid"
    ]
  }
}